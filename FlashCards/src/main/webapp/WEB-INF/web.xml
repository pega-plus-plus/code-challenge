<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>FlashCards</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>CreateFlashServlet</display-name>
    <servlet-name>CreateFlashServlet</servlet-name>
    <servlet-class>com.abc.servlets.CreateFlashServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateFlashServlet</servlet-name>
    <url-pattern>/CreateFlashServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FlashOptionServlet</display-name>
    <servlet-name>FlashOptionServlet</servlet-name>
    <servlet-class>com.abc.servlets.FlashOptionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlashOptionServlet</servlet-name>
    <url-pattern>/FlashOptionServlet</url-pattern>
  </servlet-mapping>
</web-app>